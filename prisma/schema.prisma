generator client {
  provider = "prisma-client-js"
}



datasource db {
  provider = "mysql"
  url      = "mysql://root@localhost:3306/notification"
  
}

// Aqui Ã© criado a estrutura da tabela
model Notification {
  id           String @id
  recipientId  String 
  content      String
  category     String
  createdAt    DateTime @default(now())
  canceledAt   DateTime?
  readAt       DateTime?
@@index([recipientId]) 
}

